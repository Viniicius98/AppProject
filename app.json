{
  "expo": {
    "name": "Emerj App",
    "slug": "emerj-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_emerj_principal.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo_emerj_principal.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
